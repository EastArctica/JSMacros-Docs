      "signature": "pos(x1, y1, z1, x2, y2, z2)",
      "returnType": "Box.Builder",
      "description": "Sets the box position using two opposite corners in 3D space.",
      "parameters": [
        {
          "name": "x1",
          "type": "double",
          "description": "X coordinate of first corner"
        },
        {
          "name": "y1",
          "type": "double",
          "description": "Y coordinate of first corner"
        },
        {
          "name": "z1",
          "type": "double",
          "description": "Z coordinate of first corner"
        },
        {
          "name": "x2",
          "type": "double",
          "description": "X coordinate of second corner"
        },
        {
          "name": "y2",
          "type": "double",
          "description": "Y coordinate of second corner"
        },
        {
          "name": "z2",
          "type": "double",
          "description": "Z coordinate of second corner"
        }
      ],
      "static": false,
      "examples": [
        "const box = draw3D.box()\n    .pos(0, 64, 0, 10, 74, 10)\n    .build();"
      ]
    },
